{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": "../",
  "testMatch": [
    "**/test/*.spec.ts"
  ],
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "collectCoverageFrom": [
    "src/**/*.(t|j)s",
    "!test/integrationUtils.ts"
  ],
  "coverageDirectory": "./coverage",
  "testEnvironment": "node"
}